services:
  adminer:
    container_name: adminer
    image: adminer
    networks:
      constructions: {}
    ports:
    - published: 8080
      target: 8080
    restart: always
  constructionreact:
    build:
      context: C:\Users\User\Documents\Repositorys\ConstructionReactWebServer
      dockerfile: ConstructionReact/Dockerfile
    container_name: constructionreact
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: constructionreact
    networks:
      constructions: {}
    ports:
    - target: 80
    - target: 443
    volumes:
    - C:\Users\User\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\User\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
  db:
    container_name: db
    environment:
      POSTGRES_PASSWORD: example
    image: postgres
    networks:
      constructions: {}
    ports:
    - published: 5432
      target: 5432
    restart: always
version: '3.4'